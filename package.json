{
  "name": "ng2-json-editor",
  "version": "0.0.9",
  "description": "Angular2 component for editing large json documents",
  "keywords": [
    "angular2",
    "typescript",
    "json",
    "marc",
    "json-editor",
    "ng2"
  ],
  "author": "Harun Urhan <harun.urhan@cern.ch>",
  "homepage": "https://github.com/inspirehep/ng2-json-editor",
  "license": "GPL-2.0",
  "angular-cli": {},
  "main": "ng2-json-editor.js",
  "scripts": {
    "build": "gulp build",
    "start": "npm run build && ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test:unit": "ng test --watch=false",
    "test": "npm-run-all build test:unit e2e:travis",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "e2e:travis": "npm-run-all --parallel -r start e2e:travis:delay",
    "e2e:travis:delay": "sleep 15 && npm run e2e",
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push upstream && git push upstream --tags",
    "coveralls": "cat coverage/coverage.lcov | coveralls"
  },
  "dependencies": {
    "@angular/common": "~2.1.0",
    "@angular/compiler": "~2.1.0",
    "@angular/core": "~2.1.0",
    "@angular/forms": "~2.1.0",
    "@angular/http": "~2.1.0",
    "@angular/platform-browser": "~2.1.0",
    "@angular/platform-browser-dynamic": "~2.1.0",
    "@angular/router": "~3.1.0",
    "core-js": "^2.4.1",
    "lodash": "^4.17.2",
    "immutable": "^3.8.1",
    "ng2-bootstrap": "1.1.16",
    "rxjs": "5.0.0-beta.12",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "@types/jasmine": "^2.2.30",
    "@types/lodash": "^4.14.50",
    "@types/mousetrap": "^1.5.32",
    "angular-cli": "1.0.0-beta.15",
    "bootstrap-sass": "^3.3.7",
    "codelyzer": "~0.0.26",
    "coveralls": "^2.11.12",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-inline-ng2-template": "^3.0.1",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tslint": "^6.1.1",
    "gulp-typescript": "^2.13.6",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "lodash": "^4.17.4",
    "merge2": "^1.0.2",
    "mousetrap": "^1.6.0",
    "npm-run-all": "^4.0.1",
    "process-nextick-args": "^1.0.7",
    "protractor": "4.0.5",
    "run-sequence": "^1.2.2",
    "ts-node": "1.2.1",
    "tslint": "3.13.0",
    "typescript": "2.0.2",
    "util-deprecate": "^1.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inspirehep/ng2-json-editor.git"
  },
  "bugs": {
    "url": "https://github.com/inspirehep/ng2-json-editor/issues"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
